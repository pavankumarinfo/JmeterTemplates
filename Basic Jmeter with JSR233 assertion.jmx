            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">/*
https://www.blazemeter.com/blog/writing-jmeter-functions-in-groovy
*/

String googledomain = vars[&apos;domain&apos;]
String googlePort = vars[&apos;ports&apos;]
String googleResponse = vars[&apos;getGoogleResponse&apos;]

log.info googledomain
log.info googlePort
log.info googleResponse

if(googledomain == &apos;www.google.com&apos;){
	log.warn &apos;google domain is asserted&apos;
} else {
	log.error &apos;not google domain url&apos;
	AssertionResult.setFailureMessage(&quot;no google domain&quot;);
     AssertionResult.setFailure(true);  
}
</stringProp>
